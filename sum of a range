import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in); 
        int n=sc.nextInt(); 
        int m=sc.nextInt(); 
        int i,sum;
        for(i=n,sum=0;i<=m;i++){
            sum=sum+i; 
        }
        System.out.printf("%d",sum); 
    }
}
    
